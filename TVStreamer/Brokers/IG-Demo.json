    "DefaultHeaders": {
      "Accept": "application/json; charset=UTF-8",
      "Version": "2",
      "X-IG-API-KEY": "{{Vars.ApiKey}}"
    },
    "TimeoutSeconds": 30
  },

  "Vars": {
